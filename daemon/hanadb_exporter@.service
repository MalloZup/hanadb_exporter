[Unit]
Description=SAP HANA database metrics exporter
Documentation=https://github.com/SUSE/hanadb_exporter

[Service]
Type=simple
EnvironmentFile=/usr/etc/sysconfig/hanadb_exporter
ExecStart=/usr/bin/hanadb_exporter -c /usr/etc/hanadb_exporter/%i.json -m $METRICS_FILE

[Install]
WantedBy=multi-user.target
